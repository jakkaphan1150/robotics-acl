      creator_name: '‡∏°.‡∏à‡∏±‡∏Å‡∏£‡∏û‡∏±‡∏ô‡∏ò‡∏∏‡πå ‡∏à‡∏≥‡∏õ‡∏≤‡∏®‡∏£‡∏µ'
    };

    // Mobile menu
    document.getElementById('mobileMenuBtn').addEventListener('click', () => {
      const menu = document.getElementById('mobileMenu');
      menu.classList.toggle('hidden');
    });

    document.querySelectorAll('#mobileMenu a').forEach(link => {
      link.addEventListener('click', () => document.getElementById('mobileMenu').classList.add('hidden'));
    });

    // Classroom detail
    document.querySelectorAll('.classroom-card').forEach(card => {
      card.addEventListener('click', () => showClassroom(card.dataset.grade, card.dataset.class));
    });

    function showClassroom(grade, classNum) {
      const detail = document.getElementById('classroomDetail');
      document.getElementById('classTitle').textContent = `‡∏õ.${grade}/${classNum}`;
      document.getElementById('classHeading').textContent = `‡∏ä‡∏±‡πâ‡∏ô‡∏õ‡∏£‡∏∞‡∏ñ‡∏°‡∏®‡∏∂‡∏Å‡∏©‡∏≤‡∏õ‡∏µ‡∏ó‡∏µ‡πà ${grade} ‡∏´‡πâ‡∏≠‡∏á ${classNum}`;
      document.getElementById('creatorName').textContent = window.elementSdk?.config?.creator_name || defaultConfig.creator_name;
      detail.style.display = 'flex';
      document.body.style.overflow = 'hidden';
    }

    function closeClassroom() {
      document.getElementById('classroomDetail').style.display = 'none';
      document.body.style.overflow = 'auto';
    }

    function toggleAccordion(id) {
      const content = document.getElementById(id);
      const icon = document.getElementById(id + '-icon');
      content.classList.toggle('active');
      icon.style.transform = content.classList.contains('active') ? 'rotate(0deg)' : 'rotate(-90deg)';
    }

    // Element SDK
    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange: async (config) => {
          document.getElementById('schoolName').textContent = config.school_name || defaultConfig.school_name;
          document.getElementById('heroSubtitle').textContent = config.hero_subtitle || defaultConfig.hero_subtitle;
          document.querySelectorAll('#creatorName').forEach(el => {
            el.textContent = config.creator_name || defaultConfig.creator_name;
          });
        },
        mapToCapabilities: () => ({ recolorables: [], borderables: [], fontEditable: undefined, fontSizeable: undefined }),
        mapToEditPanelValues: (config) => new Map([
          ['school_name', config.school_name || defaultConfig.school_name],
          ['hero_subtitle', config.hero_subtitle || defaultConfig.hero_subtitle],
          ['creator_name', config.creator_name || defaultConfig.creator_name]
        ])
      });
    }
  </script>
